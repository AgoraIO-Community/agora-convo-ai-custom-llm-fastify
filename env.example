# Port to run the server on
PORT=3000

# OpenAI API Key
OPENAI_API_KEY=sk-your-openai-key
OPENAI_MODEL=gpt-4o

# Whether to use the OpenAI Responses API instead of the Chat Completions API
USE_RESPONSES_API=false

# Agora App Configuration
AGORA_APP_ID=your-agora-app-id
AGORA_APP_CERTIFICATE=your-agora-app-certificate
AGORA_CUSTOMER_ID=your-agora-customer-id
AGORA_CUSTOMER_SECRET=your-agora-customer-secret

# Agent ID - used to identify the AI agent
AGENT_ID=agent_0 